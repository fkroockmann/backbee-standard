<ul{% if is_header %} id="nav-primary"{% endif %} class="nav navbar-nav nav-primary" role="navigation">
    {% if is_header %}
    <li>
        <a href="{{ this.getUri('/') }}" class="navbar-brand{% if this.getCurrentPage().isRoot() %} nav-selected{% endif %}">
            <i class="fa fa-home"></i><span class="visuallyhidden">Accueil</span>
        </a>
    </li>
    {% endif %}
    {% for item in sections %}
        {% if 1 == item.getLevel() %}
            <li{% if selected and selected.getUid() == item.getUid() %} class="nav-selected"{% endif %} role="presentation">
                <a href="{{ this.getUri(item.getUrl()) }}">{{ item.getTitle() }}</a>
            </li>
        {% endif %}
    {% endfor %}
</ul>
<!-- /-->

